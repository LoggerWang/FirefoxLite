<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Giggle</ShortName>
<InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" method="GET" template="https://gggle.co/api/geo/suggestions">
  <Param name="q" value="{searchTerms}"/>
</Url>
<!-- this is effectively x-moz-phonesearch, but search service expects a text/html entry -->
<Url type="text/html" method="GET" template="https://gggle.co/search">
  <Param name="q" value="{searchTerms}" />
</Url>
<SearchForm>https://gggle.co/</SearchForm>
</SearchPlugin>